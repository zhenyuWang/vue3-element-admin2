{
  "name": "vue3-element3-admin",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build:uat": "vite build --mode uat",
    "build": "vite build",
    "serve": "vite preview --port 8888",
    "lint": "eslint --ext .js,ts,vue src/** --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .js,ts,vue src/** --no-error-on-unmatched-pattern --fix",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-ci": "cypress run",
    "test": "npm run test-unit && npm run test-e2e-ci"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verify-commit-msg.js",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "axios": "0.21.1",
    "element-plus": "^2.1.0",
    "mockjs": "1.1.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.9",
    "@babel/preset-typescript": "7.14.5",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.10",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "3.0.5",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "2.0.0-rc.11",
    "autoprefixer": "10.2.5",
    "babel-jest": "26.6.3",
    "cross-env": "7.0.3",
    "cypress": "8.2.0",
    "eslint": "7.20.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.6.0",
    "jest": "26.6.3",
    "lint-staged": "11.1.2",
    "postcss": "8.1.0",
    "prettier": "2.2.1",
    "sass": "1.32.8",
    "ts-jest": "26.5.6",
    "typescript": "4.3.5",
    "vite": "^2.7.2",
    "vite-plugin-mock": "2.9.4",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.29.8",
    "yorkie": "2.0.0"
  }
}
